rule parse_itol:
	input:
		"summary.csv"
	output:
		directory("itol_out")
	conda:
		"../envs/python.yaml"
	log:
		"logs/parse_itol.log"
	params:
		ignore = config["parse_itol"]["ignore"]
	shell:
		"""
		python workflow/scripts/parse_iTOL.py -d ',' --margin 10 --ignore {params.ignore} --outdir {output} {input} 2>&1>{log}
		"""
